{
  "name": "notebook2electricboogaloo",
  "version": "0.1.5",
  "main": "./index.js",
  "author": "Ty Kutcher",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "build": "electron-builder",
    "package": "electron-packager . \"Note Force\" --platform=win32 --arch=x64 --out=build/ --overwrite",
    "wbuild": "webpack"
  },
  "build": {
    "appId": "me.notebook.editor",
    "productName": "Note Force",
    "directories": {
      "output": "dist/installers"
    },
    "extraFiles": [],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "./dist/images/icon.ico",
      "fileAssociations": [
        {
          "ext": "nbf",
          "name": "Note Force",
          "description": "Write your class notes, link subjects via tags, and more!"
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "license": "license.md"
    }
  },
  "keywords": [],
  "license": "MIT",
  "description": "",
  "dependencies": {
    "node-imap": "^0.9.6",
    "notebook2electricboogaloo": "file:"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/plugin-syntax-jsx": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "babel-loader": "^9.1.3",
    "electron": "^25.9.8",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
